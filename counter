import { useState } from "react";

const App = () => {

  const [count, setCount] = useState(0);
  const handleClick = () => setCount(count+1)
  return (
    <div>
      <h3>Counter</h3>
      <button onClick={handleClick}>Click</button>
      <p>You have clicked {count} times</p>
    </div>
  );
}
  
export default App;
